{
  "name": "@lukso/data-provider-urlresolver",
  "version": "1.0.0",
  "type": "module",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "unbuild",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "docs": "api-extractor run --local --verbose"
  },
  "devDependencies": {
    "@lukso/tsconfig": "workspace:*",
    "eslint": "^7.32.0",
    "eslint-config-lukso": "workspace:*",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3",
    "unbuild": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@pinata/sdk": "^2.1.0",
    "@tsndr/cloudflare-worker-jwt": "^2.2.5",
    "@types/isomorphic-fetch": "^0.0.38",
    "cross-blob": "^3.0.2",
    "cross-formdata": "^1.0.3",
    "form-data": "^4.0.0",
    "ipfs-http-client": "^60.0.1",
    "isomorphic-fetch": "^3.0.0"
  }
}
